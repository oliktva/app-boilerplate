{
  "name": "app-boilerplate",
  "version": "0.0.1",
  "description": "This is a frontend part for an app based on React, Redux, Webpack, TS, Stylus.",
  "main": "index.js",
  "scripts": {
    "build-server": "webpack --mode development --config webpack.server.config.js",
    "server": "npm run build && nodemon build/server.js",
    "prod": "npm install && npm run server",
    "dev": "npm install && webpack-dev-server --mode development --config webpack.client.config.js",
    "build": "npm install && webpack --mode production --config webpack.server.config.js"
  },
  "author": "Olga Kotova",
  "license": "ISC",
  "private": true,
  "devDependencies": {

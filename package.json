    "css-loader": "1.0.0",
    "csso-webpack-plugin": "1.0.0-beta.12",
    "eslint": "5.6.1",
    "eslint-plugin-react": "7.11.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "nodemon": "1.18.9",
    "null-loader": "0.1.1",
    "postcss-loader": "3.0.0",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "typescript": "3.3.3",
    "uglifyjs-webpack-plugin": "2.1.1",
